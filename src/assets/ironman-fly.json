        </span><span><span class="hljs-keyword">const</span></span><span> rect = card.</span><span><span class="hljs-title function_">getBoundingClientRect</span></span><span>();
        </span><span><span class="hljs-keyword">const</span></span><span> x = e.</span><span><span class="hljs-property">clientX</span></span><span> - rect.</span><span><span class="hljs-property">left</span></span><span>;
        </span><span><span class="hljs-keyword">const</span></span><span> y = e.</span><span><span class="hljs-property">clientY</span></span><span> - rect.</span><span><span class="hljs-property">top</span></span><span>;
        card.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">transform</span></span><span> = </span><span><span class="hljs-string">`rotateX(<span class="hljs-subst">${-(y - rect.height / <span class="hljs-number">2</span></span></span></span><span>) / </span><span><span class="hljs-number">15</span></span><span>}deg) rotateY(</span><span><span class="hljs-subst">${(x - rect.width / <span class="hljs-number">2</span></span></span><span>) / </span><span><span class="hljs-number">15</span></span><span>}deg)`;
      });
      card.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"mouseleave"</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
        card.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">transform</span></span><span> = </span><span><span class="hljs-string">`rotateX(0deg) rotateY(0deg)`</span></span><span>;
      });
    });
  }, [movies]);

  </span><span><span class="hljs-keyword">return</span></span><span> (
    </span><span><span class="language-xml"><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"relative min-h-screen bg-gradient-to-tr from-[#0f2027] via-[#203a43] to-[#2c5364] px-8 py-12 overflow-hidden text-white"</span></span><span>&gt;
