                </span><span><span class="hljs-attr">src</span></span><span>=</span><span><span class="hljs-string">{baseUrl</span></span><span> + </span><span><span class="hljs-attr">movie.poster_path</span></span><span>}
                </span><span><span class="hljs-attr">alt</span></span><span>=</span><span><span class="hljs-string">{movie.title}</span></span><span>
